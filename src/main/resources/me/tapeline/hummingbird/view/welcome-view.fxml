<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="37.0" layoutY="37.0" text="Welcome to Hummingbird IDE" AnchorPane.leftAnchor="32.0" AnchorPane.topAnchor="32.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <BorderPane layoutY="89.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="90.0">
         <top>
            <HBox maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="newProjectButton" mnemonicParsing="false" text="New Project" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="16.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="openProjectButton" mnemonicParsing="false" text="Open Project" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="16.0" />
                     </HBox.margin></Button>
                  <Button fx:id="settingsButton" mnemonicParsing="false" text="Settings" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="16.0" />
                     </HBox.margin></Button>
                  <Button fx:id="exitButton" mnemonicParsing="false" text="Exit" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="32.0" right="32.0" top="8.0" />
               </padding>
            </HBox>
         </top>
         <center>
            <VBox fx:id="lastProjects" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
